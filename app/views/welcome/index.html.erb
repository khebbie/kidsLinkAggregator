<script type="text/javascript">
$(document).ready(function() {
    $.getJSON('/approved.json', function(data) {
      var catIds  = [ 1, 2, 3, 4, 5 ];
      $('.result').append('<p><ul>');
      jQuery.each(catIds, function(i, catid) {
        $.each(data, function(index, value) {
         debugger; 
          if(value.link.category_id == catid)
            $('.result').append('<li><a href="' + value.link.url + '" target="_blank">' + value.link.name + '</a></li>');
          $('.result').append('</ul></p>');
      });
    });


  });
    });

</script>
